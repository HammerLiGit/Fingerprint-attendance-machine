<ContentDialog x:ConnectionId='1'
    xmlns:local="using:PAM"
    x:Class="PAM.FingerprintCollectDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" Height="400" Width="800"
    Title=""
    SecondaryButtonText="取   消" FontSize="22"                                                      
    MinWidth="800" MinHeight="400" MaxWidth="800" MaxHeight="400" Background="#FF1E3B9B" BorderBrush="#FF330661" FocusVisualSecondaryBrush="#B23C0C76" Margin="0,40,0,0" HorizontalAlignment="Stretch"
    >
    <Grid MinHeight="360">
        <Border x:ConnectionId='2' x:Name="brdOne" BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="220" Margin="56,53,0,0" VerticalAlignment="Top" Width="179" Background="White">
            <Image x:ConnectionId='7' x:Name="image1" Source="Assets/Fingerprint.png"/>
        </Border>
        <Border x:ConnectionId='3' x:Name="brdTwo" BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="220" Margin="283,53,0,0" VerticalAlignment="Top" Width="179" Background="White">
            <Image x:ConnectionId='6' x:Name="image2" Source="Assets/Fingerprint.png"/>
        </Border>
        <Border x:ConnectionId='4' x:Name="brdThree" BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="220" Margin="505,53,0,0" VerticalAlignment="Top" Width="179" Background="White">
            <Image x:ConnectionId='5' x:Name="image3" Source="Assets/Fingerprint.png"/>
        </Border>
    </Grid>



</ContentDialog>

